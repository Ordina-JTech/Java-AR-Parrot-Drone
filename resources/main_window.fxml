<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyPressed="#onKeyPressed" onKeyReleased="#onKeyReleased" prefHeight="498.0" prefWidth="1099.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.ordina.jtech.arjava.gui.Controller">
    <Button fx:id="inputConnect" layoutX="20" layoutY="20" onAction="#inputConnectToDrone" prefHeight="47.0" prefWidth="120.0" text="Connect" />
    <Button fx:id="inputDisconnect" disable="true" layoutX="160.0" layoutY="20.0" onAction="#inputDisconnectFromDrone" prefHeight="47.0" prefWidth="120.0" text="Disconnect" />
    <Button fx:id="inputTakeOff" disable="true" layoutX="300.0" layoutY="20.0" onAction="#inputDroneTakeOff" prefHeight="47.0" prefWidth="120.0" text="Take off" />
    <Button fx:id="inputLand" disable="true" layoutX="440.0" layoutY="20.0" onAction="#inputDroneLand" prefHeight="47.0" prefWidth="120.0" text="Land" />
    <Button fx:id="inputStartCamera" disable="true" layoutX="580.0" layoutY="20.0" onAction="#inputStartCamera" prefHeight="47.0" prefWidth="120.0" text="Start Camera" />
    <Button fx:id="inputStopCamera" disable="true" layoutX="720.0" layoutY="20.0" onAction="#inputStopCamera" prefHeight="47.0" prefWidth="120.0" text="Stop Camera" />
   <CheckBox fx:id="checkboxManualControl" disable="true" layoutX="21.0" layoutY="84.0" mnemonicParsing="false" onAction="#inputManualControl" text="Manual control" />
   <Label fx:id="labelRotateCounterClockwise" layoutX="20.0" layoutY="116.0" prefHeight="45.0" prefWidth="45.0" text="⟲">
      <font>
         <Font size="61.0" />
      </font>
   </Label>
    <Label fx:id="labelMoveForward" layoutX="105.0" layoutY="116.0" prefHeight="75.0" prefWidth="35.0" text="🡑">
        <font>
            <Font size="61.0" />
        </font>
    </Label>
    <Label fx:id="labelRotateClockwise" layoutX="178.0" layoutY="116.0" prefHeight="45.0" prefWidth="45.0" text="⟳">
        <font>
            <Font size="61.0" />
        </font>
    </Label>
    <Label fx:id="labelIncreaseHeight" layoutX="262.0" layoutY="116.0" prefHeight="45.0" prefWidth="45.0" text="⤉">
        <font>
            <Font size="61.0" />
        </font>
    </Label>
    <Label fx:id="labelMoveLeft" layoutX="20.0" layoutY="191.0" prefHeight="45.0" prefWidth="45.0" text="🡐">
        <font>
            <Font size="61.0" />
        </font>
    </Label>
    <Label fx:id="labelMoveBackwards" layoutX="105.0" layoutY="191.0" prefHeight="45.0" prefWidth="37.0" text="🡓">
        <font>
            <Font size="61.0" />
        </font>
    </Label>
    <Label fx:id="labelMoveRight" layoutX="176.0" layoutY="191.0" prefHeight="45.0" prefWidth="45.0" text="🡒">
        <font>
            <Font size="61.0" />
        </font>
    </Label>
    <Label fx:id="labelDecreaseHeight" layoutX="262.0" layoutY="191.0" prefHeight="45.0" prefWidth="45.0" text="⤈">
        <font>
            <Font size="61.0" />
        </font>
    </Label>
   <ImageView fx:id="imageViewCamera" fitHeight="360.0" fitWidth="640.0" layoutX="360.0" layoutY="84.0" pickOnBounds="true" preserveRatio="true" />
</AnchorPane>
